{
  "name": "REACT-WEBPACK-PROJECT-TEMPLATE",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run clean-and-copy",
    "start": "webpack-dev-server --config webpack.config.js --content-base public",
    "prebuild": "npm run clean-and-copy",
    "build": "webpack --config webpack.config.prod.js -p",
    "copy-css": "cpy src/styles/*.css public/styles",
    "copy-favicon": "cpy src/favicon.ico public",
    "copy-files": "npm run copy-css && npm run copy-favicon",
    "clean-and-copy": "npm run clean-public && npm run create-dirs && npm run copy-files",
    "clean-public": "rimraf public",
    "create-dirs": "mkdir -p public/{js,styles}",
    "test": "karma start",
    "validate": "npm-run-all --parallel validate-webpack:* test",
    "validate-webpack:dev": "webpack-validator webpack.config.js",
    "validate-webpack:prod": "webpack-validator webpack.config.prod.js",
    "watch:test": "npm test -- --auto-watch --no-single-run"
  },
  "author": "",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "path": "^0.12.7",
    "react": "^0.14.5",
    "react-dom": "^0.14.5"
  },
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.5.0",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.23.1",
    "html-webpack-plugin": "^2.19.0",
    "karma": "^1.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "node-sass": "^3.7.0",
    "npm-run-all": "^2.2.2",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.12.1",
    "webpack-validator": "^2.2.0"
  }
}
